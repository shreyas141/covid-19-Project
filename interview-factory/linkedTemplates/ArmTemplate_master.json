{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "interview-factory"
		},
		"ls_ablob_covidreportingblb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidreportingblb'"
		},
		"ls_adls_covidreportingg2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidreportingg2'"
		},
		"ls_interview_storage_g2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_interview_storage_g2'"
		},
		"ls_on_prem_oracle_sql_interview_db_hr_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_on_prem_oracle_sql_interview_db_hr'"
		},
		"ls_sql_covid_database_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_covid_database'"
		},
		"ls_adls_covidreportingg2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidgentwo.dfs.core.windows.net/"
		},
		"ls_databricks_ws_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0420-025259-txhsdtii"
		},
		"ls_http_ecdc_data_git_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"ls_interview_storage_g2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://interviewstorage2024.dfs.core.windows.net/"
		},
		"tr_population_data_arrived_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/d9d62a63-6c2a-42d8-b3cb-a0f408bfeb32/resourceGroups/covid-19-project/providers/Microsoft.Storage/storageAccounts/covidblb"
		},
		"ls_olympic_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com/darshilparmar/tokyo-olympic-azure-data-engineering-project/raw/main/data/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "interview-factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covidreportingblb_connectionString": {
						"value": "[parameters('ls_ablob_covidreportingblb_connectionString')]"
					},
					"ls_adls_covidreportingg2_accountKey": {
						"value": "[parameters('ls_adls_covidreportingg2_accountKey')]"
					},
					"ls_interview_storage_g2_accountKey": {
						"value": "[parameters('ls_interview_storage_g2_accountKey')]"
					},
					"ls_sql_covid_database_connectionString": {
						"value": "[parameters('ls_sql_covid_database_connectionString')]"
					},
					"ls_adls_covidreportingg2_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidreportingg2_properties_typeProperties_url')]"
					},
					"ls_databricks_ws_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricks_ws_properties_typeProperties_existingClusterId')]"
					},
					"ls_http_ecdc_data_git_properties_typeProperties_url": {
						"value": "[parameters('ls_http_ecdc_data_git_properties_typeProperties_url')]"
					},
					"ls_interview_storage_g2_properties_typeProperties_url": {
						"value": "[parameters('ls_interview_storage_g2_properties_typeProperties_url')]"
					},
					"ls_olympic_data_properties_typeProperties_url": {
						"value": "[parameters('ls_olympic_data_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "interview-factory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_on_prem_oracle_sql_interview_db_hr_connectionString": {
						"value": "[parameters('ls_on_prem_oracle_sql_interview_db_hr_connectionString')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'interview-factory_ArmTemplate_0')]"
			]
		},
		{
			"name": "interview-factory_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_population_data_arrived_properties_typeProperties_scope": {
						"value": "[parameters('tr_population_data_arrived_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'interview-factory_ArmTemplate_1')]"
			]
		}
	]
}